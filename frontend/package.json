{
  "name": "launcher-gui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "test:install": "playwright install firefox chromium",
    "test": "web-test-runner --config web-test-runner-ci-browsers.js",
    "test:watch": "web-test-runner --watch --playwright --browsers chromium",
    "test:chrome": "web-test-runner --config web-test-runner-chrome.js",
    "test:watch:chrome": "web-test-runner --watch --config web-test-runner-chrome.js",
    "start": "npm run build:mock && web-dev-server",
    "start:only-local": "web-dev-server",
    "serveBuild": "node dev/serveBuild.js",
    "build:mock": "cd ../mock/domain-ui-generic && npm run build:dev",
    "build:dev": "rollup -c",
    "build:prod": "rollup -c --environment BUILD:production",
    "build:selenium": "rollup -c --environment BUILD:production --environment VERSION:__VERSION__",
    "srv": "node server",
    "nuke": "rm -rf node_modules && rm -rf build",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .eslintignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .eslintignore",
    "lint:prettier": "prettier \"**/*.{js,html}\" --check --ignore-path .eslintignore",
    "format:prettier": "prettier \"**/*.{js,html}\" --write --ignore-path .eslintignore",
    "e2e:local": "wdio test_js/config/wdio.conf.js --local --local-mockserver --spec-reporter",
    "e2e:local-docker": "wdio test_js/config/wdio.conf.js --selenium-hub --local-mockserver --spec-reporter",
    "e2e:CI": "wdio test_js/config/wdio.conf.js --ci --selenium-hub --network-config-from-env  --spec-reporter --allure-reporter",
    "e2e:generateReport": "node scripts/test_js-generate-report.js",
    "openReport": "allure open test_js/allure-report",
    "sonar-scanner": "sonar-scanner"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adp/auth": "^0.1.20",
    "@adp/ui-common": "^0.1.17",
    "@adp/ui-components": "^0.1.37",
    "@adp/ui-settings": "^0.1.7",
    "@eui/action-framework": "^0.1.1",
    "@eui/app": "^1.0.0",
    "@eui/base": "^1.7.8",
    "@eui/component": "^1.0.4",
    "@eui/container": "^1.2.2",
    "@eui/layout": "^1.2.11",
    "@eui/lit-component": "^1.1.1",
    "@eui/navigation-menu": "^1.1.4",
    "@eui/theme": "^1.1.8",
    "@webcomponents/scoped-custom-element-registry": "^0.0.9",
    "es-module-shims": "^1.8.3",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "urlpattern-polyfill": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/eslint-parser": "7.24.1",
    "@babel/plugin-syntax-import-assertions": "^7.24.1",
    "@eui/rollup-config-generator": "^1.2.0",
    "@open-wc/testing": "^4.0.0",
    "@open-wc/testing-helpers": "^3.0.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@wdio/allure-reporter": "^8.36.1",
    "@wdio/cli": "^8.36.1",
    "@wdio/local-runner": "^8.36.1",
    "@wdio/mocha-framework": "^8.36.1",
    "@wdio/spec-reporter": "^8.36.1",
    "@web/dev-server": "^0.4.4",
    "@web/dev-server-import-maps": "^0.2.0",
    "@web/dev-server-rollup": "^0.6.1",
    "@web/test-runner": "^0.18.1",
    "@web/test-runner-junit-reporter": "0.7.1",
    "@web/test-runner-playwright": "^0.11.0",
    "allure-commandline": "^2.28.0",
    "chai": "^5.1.0",
    "chai-sorted": "^0.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-lit-a11y": "4.1.2",
    "express": "4.19.2",
    "fetch-mock": "^9.11.0",
    "glob": "^10.3.12",
    "koa-mount": "^4.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.0",
    "prettier": "3.2.5",
    "rollup-plugin-gzip": "^3.1.2",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-string": "^3.0.0",
    "sinon": "17.0.1",
    "sonarqube-scanner": "^3.4.0"
  }
}
